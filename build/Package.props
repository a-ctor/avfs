<Project>

  <PropertyGroup>
    <Authors>Patrick Widauer (a-ctor)</Authors>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>A Virtual File System for .NET</Description>
    <Copyright>Copyright (c) Patrick Widauer 2023</Copyright>
    <EmbedAllSources>true</EmbedAllSources>
    <PackageProjectUrl>https://github.com/a-ctor/avfs</PackageProjectUrl> 
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageLicenseExpression>ISC</PackageLicenseExpression>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <PackageIcon>Icon.png</PackageIcon> 
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>vfs;filesystem;virtual</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All"/>
  </ItemGroup>

  <ItemGroup>
    <Content Include="..\README.md" PackagePath="README.md" Visible="False"/>
    <Content Include="..\Avfs\Icon.png" PackagePath="Icon.png" Visible="False"/>
  </ItemGroup>
  
</Project>
